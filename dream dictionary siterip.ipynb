    "https://cdn.sanity.io/images/tafic6ze/production/091302948867e8810cea49c43500179441908cb4-5760x3840.jpg?w=768&amp;h=512&amp;auto=format 768w,\n",
    "https://cdn.sanity.io/images/tafic6ze/production/091302948867e8810cea49c43500179441908cb4-5760x3840.jpg?rect=1,0,5757,3840&amp;w=1024&amp;h=683&amp;auto=format 1024w,\n",
    "https://cdn.sanity.io/images/tafic6ze/production/091302948867e8810cea49c43500179441908cb4-5760x3840.jpg?rect=1,0,5758,3840&amp;w=1366&amp;h=911&amp;auto=format 1366w,\n",
    "https://cdn.sanity.io/images/tafic6ze/production/091302948867e8810cea49c43500179441908cb4-5760x3840.jpg?rect=4,0,5753,3840&amp;w=1600&amp;h=1068&amp;auto=format 1600w,\n",
    "https://cdn.sanity.io/images/tafic6ze/production/091302948867e8810cea49c43500179441908cb4-5760x3840.jpg?rect=2,0,5756,3840&amp;w=1920&amp;h=1281&amp;auto=format 1920w,\n",
